#include <iostream>

using namespace std;

int main() {
	string str;
	cin>>str;
// 	cout<<str[2]<<endl;
	int counter = 0;
	std::string::iterator it;
	for(it=str.begin();it!=str.end();it++){
	    counter++;
	}
	cout<<counter<<endl;
	cout<<"-------"<<endl;
	
	for(int i=0;i<counter;i++){
	    cout<<str[i]<<endl;
	}
}
